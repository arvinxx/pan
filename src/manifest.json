{
  "compatibleVersion": 3,
  "bundleVersion": 1,
  "icon": "icons/logo.png",
  "commands": [
    {
      "name": "System Info",
      "identifier": "system-info",
      "script": "./sketch/commands/systemInfo.ts",
      "shortcut": "cmd j"
    },
    {
      "name": "生成画板总览",
      "identifier": "artboard.overview",
      "script": "./sketch/app.ts",
      "shortcut": "ctrl a",
      "handler": "artboardOverview"
    },
    {
      "name": "交换位置",
      "identifier": "swap-position",
      "script": "./sketch/app.ts",
      "shortcut": "ctrl shift s",
      "handler": "swapPosition"
    },
    {
      "name": "交换文本",
      "identifier": "swap-text",
      "script": "./sketch/app.ts",
      "shortcut": "ctrl shift s",
      "handler": "swapText"
    },
    {
      "name": "Home",
      "identifier": "win.home",
      "script": "./sketch/app.ts",
      "handler": "home"
    },
    {
      "name": "查找替换",
      "identifier": "win.replace",
      "script": "./sketch/app.ts",
      "handler": "replace",
      "shortcut": "ctrl k"
    },
    {
      "name": "打开表格",
      "identifier": "win.table",
      "script": "./sketch/app.ts",
      "handler": "table",
      "shortcut": "ctrl t"
    }
  ],
  "menu": {
    "title": "🚀🚀Pan🚀🚀",
    "items": [
      "system-info",
      "artboard.overview",
      "panel",
      "win.replace",
      "win.table",
      {
        "title": "交换",
        "items": ["swap-position", "swap-text"]
      }
    ]
  }
}
