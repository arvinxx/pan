{
  "compatibleVersion": 3,
  "bundleVersion": 1,
  "icon": "icons/logo.png",
  "commands": [
    {
      "name": "系统信息",
      "identifier": "com.arvinxx.pan.system-info",
      "script": "./sketch/commands/systemInfo.ts",
      "shortcut": "cmd j"
    },
    {
      "name": "生成画板总览",
      "identifier": "com.arvinxx.pan.artboards-overview",
      "script": "./sketch/commands/artboardsOverview.ts",
      "shortcut": "ctrl a"
    },
    {
      "name": "交换位置",
      "identifier": "com.arvinxx.pan.swap-position",
      "script": "./sketch/commands/swap.ts",
      "shortcut": "ctrl shift s",
      "handler": "swapPosition"
    },
    {
      "name": "交换文本",
      "identifier": "com.arvinxx.pan.swap-text",
      "script": "./sketch/commands/swap.ts",
      "shortcut": "ctrl shift s",
      "handler": "swapText"
    },
    {
      "name": "Home",
      "identifier": "com.arvinxx.pan.win.home",
      "script": "./sketch/windows/home.ts"
    },
    {
      "name": "表格",
      "identifier": "com.arvinxx.pan.win.table",
      "script": "./sketch/windows/table.ts",
      "shortcut": "ctrl t"
    },
    {
      "name": "查找替换",
      "identifier": "com.arvinxx.pan.win.replace",
      "script": "./sketch/windows/replace.ts",
      "shortcut": "ctrl k"
    }
  ],
  "menu": {
    "title": "🚀🚀Pan🚀🚀",
    "items": [
      "com.arvinxx.pan.system-info",
      "com.arvinxx.pan.artboards-overview",
      "com.arvinxx.pan.panel",
      "com.arvinxx.pan.win.home",
      "com.arvinxx.pan.win.replace",
      "com.arvinxx.pan.win.table",
      {
        "title": "交换",
        "items": ["com.arvinxx.pan.swap-position", "com.arvinxx.pan.swap-text"]
      }
    ]
  }
}
