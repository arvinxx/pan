{
  "name": "pan",
  "version": "0.1.4",
  "author": "ÈóªÂÜ∞",
  "description": "Áî®Âπ≥Â∫ïÈîÖÊâçÊòØÁéãËÄÖ",
  "engines": {
    "sketch": ">=3.0"
  },
  "repository": "https://github.com/arvinxx/pan",
  "skpm": {
    "name": "üç≥Pan",
    "manifest": "src/sketch/manifest.js",
    "main": "pan.sketchplugin",
    "assets": [
      "static",
      "dist"
    ]
  },
  "scripts": {
    "dev": "concurrently \"npm run dev:resources\" \"npm run dev:sketch\"",
    "dev:sketch": "NODE_ENV=development skpm-build --watch",
    "dev:resources": "APP_ROOT=src/resources PORT=8110 BROWSER=none umi dev",
    "dev:sketch-only": "npm run build:resources &&skpm-build --watch",
    "build": "npm run build:resources&& npm run build:sketch",
    "build:sketch": "NODE_ENV=prod skpm-build",
    "build:resources": "APP_ROOT=src/resources umi build",
    "build:resources-analyze": "APP_ROOT=src/resources ANALYZE=1 umi build",
    "refresh": "find ./*.sketchplugin -name \"*_ts\" | entr -r /Applications/Sketch.app/Contents/MacOS/Sketch",
    "postinstall": "npm run build && skpm-link"
  },
  "devDependencies": {
    "@sketch-hq/sketch-file-format-ts": "^5.1.0",
    "@skpm/builder": "^0.7.7",
    "@types/classnames": "^2.2.10",
    "@types/mocha": "^7.0.2",
    "@typescript-eslint/eslint-plugin": "^3.1.0",
    "@typescript-eslint/parser": "^3.1.0",
    "@umijs/hooks": "^1.9.3",
    "@umijs/preset-react": "1.x",
    "@umijs/test": "^3.2.3",
    "concurrently": "^5.2.0",
    "eslint": "^7.1.0",
    "immutability-helper": "^3.1.1",
    "lint-staged": "^10.0.7",
    "prettier": "^1.19.1",
    "react": "^16.13.1",
    "react-dom": "^16.13.1",
    "react-intl": "^4.6.7",
    "sketch-module-web-view": "^3.4.1",
    "terser-webpack-plugin": "^3.0.3",
    "ts-loader": "^7.0.5",
    "typescript": "^3.9.5",
    "umi": "^3.2.3",
    "yorkie": "^2.0.0"
  },
  "dependencies": {
    "antd": "^4.3.3",
    "@ant-design/icons": "^4.2.1",
    "sketch-constants": "^1.1.0"
  }
}
